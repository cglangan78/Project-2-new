<h1>Add A Picture</h1>
  <%= form_for Picture.new do |p|  %>
  <%= p.text_field(:url, placeholder: 'URL')%> <br>
  <%= p.text_field(:summary, placeholder: 'Leave a summary')%> <br>
  <%= p.text_field(:location, placeholder: 'City')%> <br>
  <%# p.hidden_field :user_id, :value => @user.id %>
  <%= p.submit('Submit') %>
<%end%>
